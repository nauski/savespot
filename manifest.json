{
  "manifest_version": 3,
  "name": "Custom Download Folder",
  "version": "1.0",
  "permissions": ["downloads", "contextMenus"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}
